   "source": [
    "def countWays(n):\n",
    "    if(n < 0):\n",
    "        return 0\n",
    "    elif n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return countWays(n-1) + countWays(n-2) + countWays(n-3)"
   ]
